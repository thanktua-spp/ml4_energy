{
  "name": "ML4 Energy",
  "build": {
    "dockerfile": "./Dockerfile",
    "context": ".."
  },
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash",
    "python.pythonPath": "/app/venv/bin/python"
  },
  "postCreateCommand": "bash setup/setup.sh",
  "remoteUser": "root",
  "workspaceFolder": "/app",
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter"
  ]
}
